{
  "router": {
    "routes": [
      {
        "name": "audit",
        "target_url": "http://opensearch:9200",
        "matchers": [
          {
            "condition": "path",
            "pattern": "/audit/.*",
            "is_regex": true
          }
        ],
        "priority": 10,
        "strip_prefix": "/audit"
      },
      {
        "name": "guardrails-admin",
        "target_url": "http://guardrails:8001",
        "matchers": [
          {
            "condition": "path",
            "pattern": "/guardrails/.*",
            "is_regex": true
          }
        ],
        "priority": 15
      },
      {
        "name": "agent",
        "target_url": "http://cube-agent:8901",
        "default_rule": true
      }
    ]
  },
  "tls": {
    "enabled": true,
    "insecure_skip_verify": false,
    "min_version": 771,
    "max_version": 772
  }
}